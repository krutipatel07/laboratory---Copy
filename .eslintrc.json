{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  // "extends": "next/core-web-vitals",
  "extends": [
    "standard-react",
    "next/core-web-vitals"
],
  "parserOptions": {
    "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "@next/next/no-img-element": "off",
    "jsx-a11y/alt-text": "off",
    "react/display-name": "off",
    "react/jsx-max-props-per-line": [
      1,
      {
        "maximum": 1
      }
    ]
  }
}